{
  "agents": {
    "Sisyphus": {
      "model": "openrouter/{{ORCHESTRATOR_MODEL}}"
    },
    "Genius": {
      "model": "openrouter/{{GENIUS_MODEL}}"
    },
    "oracle": {
      "model": "openrouter/{{PLANNER_MODEL}}",
      "permissions": {
        "allow_tool_execution": "ask"
      }
    },
    "librarian": {
      "model": "openrouter/{{LIBRARIAN_MODEL}}"
    },
    "explore": {
      "model": "openrouter/{{LIBRARIAN_MODEL}}"
    },
    "frontend-ui-ux-engineer": {
      "model": "openrouter/{{ORCHESTRATOR_MODEL}}"
    },
    "document-writer": {
      "model": "openrouter/{{LIBRARIAN_MODEL}}"
    },
    "multimodal-looker": {
      "model": "openrouter/{{LIBRARIAN_MODEL}}"
    },
    "Metis (Plan Consultant)": {
      "model": "openrouter/{{PLANNER_MODEL}}"
    },
    "Momus (Plan Reviewer)": {
      "model": "openrouter/{{PLANNER_MODEL}}"
    }
  },
  "sisyphus_agent": {
    "max_iterations": {{ULTRAWORK_MAX_ITERATIONS}},
    "auto_recover": false
  },
  "tool_permissions": {
    "bash": {
      "mode": "{{BASH_PERMISSION_MODE}}",
      "allowed_commands": {{BASH_ALLOWED_COMMANDS}},
      "blocked_commands": {{BASH_BLOCKED_COMMANDS}},
      "blocked_patterns": {{BASH_BLOCKED_PATTERNS}}
    }
  }
}
